Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"BayesianProbability", "[", 
     RowBox[{
     "trajectory_List", ",", "outcomes_List", ",", "nrSignals_Integer", ",", 
